{{#partial "styles"}}
    <link rel="stylesheet" href="/css/home.css">
{{/partial}}

{{#partial "content"}}
    <main>

        <article class="don">
            <div class="turtle">
                <div class="head">
                    <div class="stiches">
                        <div class="stich"></div>
                        <div class="stich"></div>
                        <div class="stich"></div>
                        <div class="stich"></div>
                        <div class="stich"></div>
                        <div class="stich"></div>
                        <div class="stich"></div>
                    </div>
                    <div class="bandana"></div>
                    <div class="eyes">
                        <div class="eye is-left"></div>
                        <div class="eye is-right">

                        </div>
                    </div>
                    <div class="mouth"></div>
                </div>

                <div class="body-frame">
                    <div class="belt"></div>
                    <div class="chest">
                        <div class="shell">
                            <span></span>
                            <span></span>
                            <span></span>
                            <span></span>
                        </div>
                    </div>

                    <div class="leg is-left"></div>
                    <div class="leg is-right"></div>
                </div>
                <div class="arm left">
                    <span></span>
                </div>
                <div class="arm right">
                    <span></span>
                </div>
                <div class="stick"></div>
                <div class="half-shell">

                </div>

                <div class="leg left">
                    <div class="inside-leg"></div>

                </div>
                <div class="leg right">
                    <div class="inside-leg"></div>

                </div>
            </div>
        </article>

    </main>
<div class="row">
    <div class="col-sm-6">
        <div class="card">
            <img class="card-img-top" src="/images/justice-league.png" alt="Card image cap">
            <div class="card-body">
                <h5 class="card-title">Add A Squad</h5>
                <p class="card-text t-2">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
                <a href="/squads" class="btn btn-primary t-2">Squads</a>
            </div>
        </div>
    </div>
    <div class="col-sm-6">
        <div class="card">
            <img class="card-img-top" src="/images/spider-man.png" alt="Card image cap">
            <div class="card-body">
                <h5 class="card-title">Add a Hero</h5>
                <p class="card-text t-2">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
                <a href="/heroes" class="btn btn-primary t-2">Add hero</a>
            </div>
        </div>
    </div>
</div>
{{/partial}}

{{> layout.hbs}}